{
  "log": {
    "level": "fatal",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "Internet-dns",
        "address": "9.9.9.9",
        "address_strategy": "prefer_ipv6",
        "strategy": "prefer_ipv6",
        "detour": "Internet"
      },
      {
        "tag": "direct-dns",
        "address": "local",
        "address_strategy": "prefer_ipv6",
        "strategy": "prefer_ipv6",
        "detour": "direct-out"
      },
      {
        "tag": "remote",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "direct-dns"
      },
      {
        "domain_suffix": [
          ".lan"
        ],
        "server":"direct-dns"
      },
      {
        "rule_set": ["geoip-private"],
        "server":"direct-dns"
      },
      {
        "query_type": [
          "A",
          "AAAA",
          "HTTPS",
          "SVCB"
        ],
        "server": "remote"
      }
    ],
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "strategy": "prefer_ipv6",
    "independent_cache": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "yacd",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "Internet",
      "default_mode": "rule"
    }
  }
}